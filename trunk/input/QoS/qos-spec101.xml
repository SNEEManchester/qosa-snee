<?xml version="1.0"?>

<qos-specification
xmlns="http://dias-mc.cs.manchester.ac.uk"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://dias-mc.cs.manchester.ac.uk qos-specification.xsd">

	<expectations>

		<optimization-goal>
			<type>MINIMIZE</type>
			<variable>ACQUISITION_INTERVAL</variable>
			<weighting>1</weighting>
		</optimization-goal>

		<delivery-time>
			<units>SECONDS</units>
			<constraint>
				<less-equals>90</less-equals>
			</constraint>
		</delivery-time>

	</expectations>
	
	<query-duration>
		<units>SECONDS</units>
		<equals>60</equals>
	</query-duration>
			
</qos-specification>	

